{
	"version": 2,
	"name": "The Loneliness Project",
	"alias": "TheLonelinessProject.colinrumball.com",
	"builds": [
		{
			"src": "/server/index.ts",
			"use": "@now/node-server"
		},
		{
			"src": "/static/**",
			"use": "@now/static"
		},
		{
			"src": "/functions/*.ts",
			"use": "@now/node"
		},
		{
			"src": "package.json",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/graphql",
			"dest": "/server/index.ts"
		},
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/functions/(.*)",
			"dest": "/functions/$1"
		}
	]
}
